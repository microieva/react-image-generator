{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../node_modules",
    "paths": {
      "*": ["*", "../node_modules/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}